{
  "collectCoverage": true,
  "globals": { "widow": true },
  "transform": {
    "^.+\\.js$": "babel-jest"
  },
  "moduleFileExtensions": [
    "js",
    "jsx"
  ],
  "moduleDirectories": ["node_modules", "src", "src/app", "src/app/actions", "__mocks__"],
  "moduleNameMapper": {
    "^.+\\.(css|scss)$": "identity-obj-proxy"
  },
  "setupFiles": ["raf/polyfill", "jest-localstorage-mock"],
  "setupTestFrameworkScriptFile": "<rootDir>/src/__tests__/setup.js",
  "testRegex": "/__tests__/.*.test.js$",
  "testPathIgnorePatterns": ["<rootDir>/src/__tests__/coverage"],
  "coveragePathIgnorePatterns": ["<rootDir>/src/__tests__", "<rootDir>/src/__mocks__",
    "<rootDir>/src/default", "<rootDir>/src/app/constants", "<rootDir>/src/app/api"]
}



